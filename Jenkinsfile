   agent any
    stages{
      stage("build"){
        steps{
         echo "this is balaiah"
         }
        }
        stage("test"){
         steps{
           echo "this is sudha"
       }
      }
      stage("deploy"){
       steps{
        echo "this is karuna"
        }
        }
        stage("release"){
        steps{
          echo "this is sunny"
          }
          }
          }
          }
